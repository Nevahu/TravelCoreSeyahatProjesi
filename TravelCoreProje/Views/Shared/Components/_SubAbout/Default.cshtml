@using EntityLayer.Concrete
@model List<SubAbout>
<section class="w3l-bottom py-5">
    <div class="container py-md-4 py-3 text-center">
        <div class="row my-lg-4 mt-4">
            <div class="col-lg-9 col-md-10 ml-auto">
                <div class="bottom-info ml-auto">
                    <div class="header-section text-left">
                        @foreach (var item in Model)
                        {
                            <h3 class="hny-title two">@item.Title</h3>
                            <p class="mt-3 pr-lg-5">
                                Hemen rezervasyon yap, dilediğin yere minimum fiyatlarla
                                konforlu bir şekilde gidip seyehatin tadını çıkar.
                                Balkanlarda Akdeniz'e, İskandinav Ülkelerinden Orta ASya'ya, 
                                Ege kıyılarında Karadenize kadar bütün rotalar için bir tık uzaktayız
                            </p>
                        }
                        <a href="about.html" class="btn btn-style btn-secondary mt-5">Read More</a>
                    </div>


                </div>
            </div>
        </div>
    </div>
</section>

